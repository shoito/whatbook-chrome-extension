        ],
        "css": [
          "styles/main.css"
        ],
        "js": [
          "components/jquery/jquery.js",
          "scripts/util.js",
          "scripts/cs-kinokuniya.js",
          "scripts/cs-common.js"
        ],
        "run_at": "document_idle",
        "all_frames": false
      }
    ],
    "permissions": [
      "tabs",
      "storage",
      "http://*/*",
      "https://*/*"
    ]
}
